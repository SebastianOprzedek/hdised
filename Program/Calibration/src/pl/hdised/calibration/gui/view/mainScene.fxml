<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" prefHeight="500.0" prefWidth="700.0" minHeight="500.0" minWidth="700.0"
          maxHeight="500.0" maxWidth="700.0" vgap="10" xmlns="http://javafx.com/javafx/8"
          xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.hdised.calibration.gui.controller.MainSceneController">
    <children>
        <TabPane fx:id="tabPane" prefHeight="500.0" prefWidth="700.0" tabClosingPolicy="UNAVAILABLE">
            <tabs>
                <Tab fx:id="readTrainingDataTab" text="Read training data"/>
                <Tab fx:id="showTrainingDataTab" text="Show training data"/>
                <Tab fx:id="readTestDataTab" text="Read test data"/>
                <Tab fx:id="showTestDataTab" text="Show test data"/>
                <Tab fx:id="neuralNetworkTab" text="Neural network"/>
                <Tab fx:id="machineLearningTab" text="Machine learning"/>
            </tabs>
        </TabPane>
    </children>
</GridPane>
